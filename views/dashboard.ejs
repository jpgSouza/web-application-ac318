<!DOCTYPE html>
<style>
    body {
        width: 100%;
        height: 100%;
        margin: 0 auto;
    }

    .menu-principal {
        width: 100%;
        background-color: #252323;
        height: 60px;
    }

    main {
        margin: 0 auto;
        width: 980px;
        position: relative;
    }

    .logo {
        float: left;
        padding: 0;
        width: 70%;
    }

    .redes-sociais ul li {
        margin-top: 16px;
        margin-left: 15px;
        display: inline-block;
        float: left;
        list-style: none;
    }

    .redes-socias {
        width: 25%;
        float: right;
    }

    .header-2 {
        background-color: #ffffff;
        width: 100%;
        border-top: 3px solid #efefef;
    }

    .menu ul li {
        display: inline-block;
        color: #8b8b8b;
        margin-left: 15px;
    }

    .menu ul li a {
        color: #8b8b8b;
        text-decoration: none;
        font-size: 18px;
        text-transform: uppercase;
    }

    .menu {
        width: 70%;
        float: left;
    }

    .busca {
        width: 30%;
        float: right;
    }

    #image {
        width: 40%;
    }

    #carouselSite {
        height: 550px;
    }

    .carousel-inner {
        height: 550px;
    }

    .carousel-caption {
        top: 50%;
    }

    .carousel-item {
        height: 50rem;
    }

    .carousel-item>img {
        position: absolute;
        top: -50%;
        left: 0;
        min-width: 100%;
    }

    img {
        vertical-align: middle;
    }

    .head {
        float: left;
        width: 100%;
    }

    .search-box {
        width: 90%;
        margin: 0 auto 40px;
        box-shadow: 10px 13px 0 rgba(0, 0, 0, 0.2);
    }

    .listing-block {
        background: #fff;
        height: 500px;
        padding-top: 20px;
        overflow-y: scroll;
    }

    .slimScrollDiv {
        width: 80% !important;
    }

    .media {
        background: #fff;
        position: relative;
        margin-bottom: 15px;
    }

    .media img {
        width: 200px;
        margin: 0;
        height: 136px;
    }

    .media-body {
        border: 1px solid #bcbcbc;
        border-left: 0;
        height: 136px;
    }

    .media .price {
        float: left;
        width: 100%;
        font-size: 30px;
        font-weight: 600;
        color: #4765AB;
    }

    .media .price small {
        display: block;
        font-size: 17px;
        color: #232323;
    }

    .media .stats {
        float: left;
        width: 100%;
        margin-top: 10px;
    }

    .media .stats span {
        float: left;
        margin-right: 10px;
        font-size: 15px;
    }

    .media .stats span i {
        margin-right: 7px;
        color: #4765AB;
    }

    .media .address {
        float: left;
        width: 100%;
        font-size: 14px;
        margin-top: 5px;
        color: #888;
    }

    .media .fav-box {
        position: absolute;
        right: 10px;
        font-size: 20px;
        top: 4px;
        color: #E74C3C;
    }

    .bg-gradient-primary {
        background: #007bff linear-gradient(180deg, #268fff, #007bff) repeat-x !important;
    }

    img {
        vertical-align: middle;
        border-style: none;
        width: 25%;
    }
</style>
